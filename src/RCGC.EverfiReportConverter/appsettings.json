{
  "ReportConfiguration": {
    "CSVReportPath": "%TMP%\\CSVFile\\",
    "ArchivePath": "%TMP%\\Archive\\",
    "ReportSavePath": "%TMP%\\Report\\"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%TMP%\\Logs\\RCGC-Everfi_Report-.log",
          "ROllingInterval": "Minute"
        }
      }
    ]
  }
}
